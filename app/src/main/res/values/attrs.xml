<resources>

    <!-- Declare custom theme attributes that allow changing which styles are
         used for button_PIN bars depending on the API level.
         ?android:attr/buttonBarStyle is new as of API 11 so this is
         necessary to support previous API levels. -->
    <declare-styleable name="ButtonBarContainerTheme">
        <attr name="metaButtonBarStyle" format="reference" />
        <attr name="metaButtonBarButtonStyle" format="reference" />
    </declare-styleable>

    <attr name="backgroundStyle" format="reference" />
    <attr name="playBackground" format="reference" />
    <attr name="floatingButtonStyle" format="reference" />
    <attr name="buttonStyles" format="reference" />
    <attr name="textStyle" format="reference" />
    <attr name="homeText" format="reference" />
    <attr name="helpText" format="reference" />
    <attr name="helpIconStyle" format="reference" />
    <attr name="aboutIconStyle" format="reference" />
    <attr name="aboutStyle" format="reference" />
    <attr name="firstImage" format="reference" />
    <attr name="firstText" format="reference" />
    <attr name="firstTextTitle" format="reference" />
    <attr name="secondImage" format="reference" />
    <attr name="secondText" format="reference" />
    <attr name="secondTextTitle" format="reference" />
    <attr name="thirdImage" format="reference" />
    <attr name="thirdText" format="reference" />
    <attr name="thirdTextTitle" format="reference" />
    <attr name="fourthImage" format="reference" />
    <attr name="fourthText" format="reference" />
    <attr name="fourthTextTitle" format="reference" />
    <attr name="preference_speaker" format="reference" />
    <attr name="preference_vibration" format="reference" />
    <attr name="preference_notification" format="reference" />
    <attr name="preference_light" format="reference" />
    <attr name="preference_player" format="reference" />
    <attr name="preference_theme" format="reference" />
    <attr name="preference_language" format="reference" />
    <attr name="backgroundDialog" format="reference" />

</resources>
